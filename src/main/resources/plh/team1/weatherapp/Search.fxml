<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox spacing="20.0" styleClass="container" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="plh.team1.weatherapp.SearchController">
    <children>
        <HBox alignment="CENTER_RIGHT" minHeight="-Infinity" prefHeight="54.0" prefWidth="0.0" spacing="20.0" stylesheets="@styles.css">
            <children>
                <Button fx:id="searchButton" onAction="#switchToOverview" stylesheets="@styles.css" text="Overview">
                    <styleClass>
                        <String fx:value="cursor-pointer" />
                        <String fx:value="menu-item" />
                    </styleClass>
                </Button>
                <Label stylesheets="@styles.css" text="Search">
                    <styleClass>
                        <String fx:value="menu-item" />
                        <String fx:value="active" />
                    </styleClass>
                </Label>
            </children>
         <styleClass>
            <String fx:value="navigation" />
            <String fx:value="divider-bottom" />
         </styleClass>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
