<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox fx:id="root" prefHeight="720.0" prefWidth="1024.0" spacing="20.0" styleClass="container" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="plh.team1.weatherapp.SearchController">
    <children>
        <HBox alignment="CENTER_RIGHT" minHeight="-Infinity" prefHeight="54.0" prefWidth="0.0" spacing="20.0" stylesheets="@styles.css">
            <children>
                <Button fx:id="searchButton" onAction="#switchToOverview" stylesheets="@styles.css" text="Overview">
                    <styleClass>
                        <String fx:value="cursor-pointer" />
                        <String fx:value="menu-item" />
                    </styleClass>
                </Button>
                <Label stylesheets="@styles.css" text="Search">
                    <styleClass>
                        <String fx:value="menu-item" />
                        <String fx:value="active" />
                    </styleClass>
                </Label>
            </children>
            <styleClass>
                <String fx:value="navigation" />
                <String fx:value="divider-bottom" />
            </styleClass>
        </HBox>
        <AnchorPane fx:id="searchWrapper" prefHeight="25.0" prefWidth="200.0">
            <children>
                <TextField fx:id="searchBar" prefHeight="30.0" promptText="Type city or country name to search..." styleClass="searchbar" stylesheets="@styles.css" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="80.0" />
                <Button mnemonicParsing="false" onMouseClicked="#clearSearch" opacity="0.7" prefHeight="30.0" prefWidth="35.0" stylesheets="@styles.css" text="x" textAlignment="CENTER" AnchorPane.rightAnchor="80.0">
                    <styleClass>
                        <String fx:value="clear-btn" />
                        <String fx:value="cursor-pointer" />
                    </styleClass>
                </Button>
                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" stylesheets="@styles.css" AnchorPane.rightAnchor="40.0">
                    <styleClass>
                        <String fx:value="action-btn" />
                        <String fx:value="cursor-pointer" />
                    </styleClass>
                    <graphic>
                        <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../icons/recent.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button graphicTextGap="0.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0" stylesheets="@styles.css" AnchorPane.rightAnchor="0.0">
                    <graphic>
                        <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../icons/favourite.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <styleClass>
                        <String fx:value="action-btn" />
                        <String fx:value="cursor-pointer" />
                    </styleClass>
                </Button>
                <ListView fx:id="cityListView" prefHeight="0.0" prefWidth="200.0" styleClass="overlay" stylesheets="@styles.css" visible="false" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0" />
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </AnchorPane>
        <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0" VBox.vgrow="ALWAYS">
            <children>
                <VBox fx:id="cityInfoWrapper" prefWidth="320.0" visible="false">
                    <children>
                        <VBox fx:id="cityInfo" styleClass="city-info-wrapper" stylesheets="@styles.css" visible="false">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0" stylesheets="@styles.css" VBox.vgrow="NEVER">
                                    <children>
                                        <Label styleClass="city-info-label" stylesheets="@styles.css" text="City" />
                                        <Pane HBox.hgrow="ALWAYS" />
                                        <Label fx:id="cityName" alignment="CENTER_RIGHT" stylesheets="@styles.css">
                                            <styleClass>
                                                <String fx:value="city-info-label" />
                                                <String fx:value="medium" />
                                            </styleClass>
                                        </Label>
                                    </children>
                                    <VBox.margin>
                                        <Insets left="10.0" right="10.0" />
                                    </VBox.margin>
                                    <styleClass>
                                        <String fx:value="city-info-label-wrapper" />
                                        <String fx:value="divider-bottom" />
                                    </styleClass>
                                    <padding>
                                        <Insets bottom="6.0" top="6.0" />
                                    </padding>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" layoutX="21.0" layoutY="17.0" prefHeight="30.0" prefWidth="200.0" stylesheets="@styles.css">
                                    <children>
                                        <Label styleClass="city-info-label" stylesheets="@styles.css" text="Country" />
                                        <Pane HBox.hgrow="ALWAYS" />
                                        <Label fx:id="cityCountry" alignment="CENTER_RIGHT" stylesheets="@styles.css">
                                            <styleClass>
                                                <String fx:value="city-info-label" />
                                                <String fx:value="medium" />
                                            </styleClass>
                                        </Label>
                                    </children>
                                    <padding>
                                        <Insets bottom="6.0" top="6.0" />
                                    </padding>
                                    <styleClass>
                                        <String fx:value="city-info-label-wrapper" />
                                        <String fx:value="divider-bottom" />
                                    </styleClass>
                                    <VBox.margin>
                                        <Insets left="10.0" right="10.0" />
                                    </VBox.margin>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0" stylesheets="@styles.css">
                                    <children>
                                        <Label prefWidth="65.0" styleClass="city-info-label" stylesheets="@styles.css" text="Longitude" />
                                        <Pane HBox.hgrow="ALWAYS" />
                                        <Label fx:id="cityLng" alignment="CENTER_RIGHT" minWidth="150.0" stylesheets="@styles.css">
                                            <styleClass>
                                                <String fx:value="city-info-label" />
                                                <String fx:value="medium" />
                                            </styleClass>
                                        </Label>
                                    </children>
                                    <padding>
                                        <Insets bottom="6.0" top="6.0" />
                                    </padding>
                                    <styleClass>
                                        <String fx:value="city-info-label-wrapper" />
                                        <String fx:value="divider-bottom" />
                                    </styleClass>
                                    <VBox.margin>
                                        <Insets left="10.0" right="10.0" />
                                    </VBox.margin>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0" stylesheets="@styles.css">
                                    <children>
                                        <Label styleClass="city-info-label" stylesheets="@styles.css" text="Latitude" />
                                        <Pane HBox.hgrow="ALWAYS" />
                                        <Label fx:id="cityLat" alignment="CENTER_RIGHT" stylesheets="@styles.css">
                                            <styleClass>
                                                <String fx:value="city-info-label" />
                                                <String fx:value="medium" />
                                            </styleClass>
                                        </Label>
                                    </children>
                                    <padding>
                                        <Insets bottom="6.0" top="6.0" />
                                    </padding>
                                    <styleClass>
                                        <String fx:value="city-info-label-wrapper" />
                                        <String fx:value="divider-bottom" />
                                    </styleClass>
                                    <VBox.margin>
                                        <Insets left="10.0" right="10.0" />
                                    </VBox.margin>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0" styleClass="city-info-label-wrapper" stylesheets="@styles.css">
                                    <children>
                                        <Label styleClass="city-info-label" stylesheets="@styles.css" text="Population" />
                                        <Pane HBox.hgrow="ALWAYS" />
                                        <Label fx:id="cityPopulation" alignment="CENTER_RIGHT" stylesheets="@styles.css">
                                            <styleClass>
                                                <String fx:value="city-info-label" />
                                                <String fx:value="medium" />
                                            </styleClass>
                                        </Label>
                                    </children>
                                    <padding>
                                        <Insets bottom="6.0" top="6.0" />
                                    </padding>
                                    <VBox.margin>
                                        <Insets left="10.0" right="10.0" />
                                    </VBox.margin>
                                </HBox>
                            </children>
                            <padding>
                                <Insets bottom="6.0" top="6.0" />
                            </padding>
                        </VBox>
                        <Pane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                        <Button alignment="CENTER" minHeight="35.0" mnemonicParsing="false" prefWidth="320.0" stylesheets="@styles.css" text="Add to favourites">
                            <VBox.margin>
                                <Insets bottom="10.0" />
                            </VBox.margin>
                            <styleClass>
                                <String fx:value="favourite-btn" />
                                <String fx:value="bold" />
                                <String fx:value="cursor-pointer" />
                            </styleClass>
                        </Button>
                        <Button alignment="CENTER" minHeight="35.0" mnemonicParsing="false" prefWidth="320.0" stylesheets="@styles.css" text="Search">
                            <styleClass>
                                <String fx:value="search-btn" />
                                <String fx:value="bold" />
                                <String fx:value="cursor-pointer" />
                            </styleClass>
                        </Button>
                    </children>
                </VBox>
                <VBox fx:id="mapViewWrapper" prefHeight="200.0" prefWidth="100.0" styleClass="city-info-wrapper" stylesheets="@styles.css" visible="false" HBox.hgrow="ALWAYS">
                    <children>
                        <WebView fx:id="mapView" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" visible="false" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
