            </rotationAxis>
        </HBox>
        <HBox alignment="TOP_RIGHT" spacing="20.0" stylesheets="@styles.css">
            <children>
            <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="24.0" prefHeight="200.0" prefWidth="340.0" prefWrapLength="200.0" vgap="24.0">
               <children>
                      <AnchorPane prefHeight="100.0" prefWidth="158.0" styleClass="stat" stylesheets="@styles.css">
