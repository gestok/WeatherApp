<VBox spacing="20.0" styleClass="container" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="plh.team1.weatherapp.OverviewController">
    <children>
        <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="54.0" styleClass="navigation" stylesheets="@styles.css">
            <children>
                <Button fx:id="menuItemOverview" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="54.0" prefWidth="54.0">
                    <graphic>
                        <ImageView fitHeight="26.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../icons/report.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <styleClass>
                        <String fx:value="menu-item" />
                        <String fx:value="report" />
                        <String fx:value="active" />
                    </styleClass>
                </Button>
                <Button fx:id="menuItemSearch" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#switchToSearch" prefHeight="54.0" prefWidth="54.0" stylesheets="@styles.css">
                    <styleClass>
                        <String fx:value="menu-item" />
                        <String fx:value="cursor-pointer" />
                        <String fx:value="search" />
                    </styleClass>
                    <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../icons/search.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets left="20.0" right="20.0" />
            </padding>
            <rotationAxis>
                <Point3D />
            </rotationAxis>
        </HBox>
        <HBox alignment="CENTER_LEFT" spacing="20.0" stylesheets="@styles.css">
            <children>
                <AnchorPane prefHeight="100.0" prefWidth="158.0" styleClass="stat" stylesheets="@styles.css">
                    <children>
                        <Label fx:id="uvindex_v" styleClass="stat-value" stylesheets="@styles.css" text="..." />
                        <ImageView fitWidth="50.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="-8.0">
                            <image>
                                <Image url="@../../../gfx/uvindex.png" />
                            </image>
                        </ImageView>
                        <Label styleClass="stat-title" stylesheets="@styles.css" text="UV Index" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                            <effect>
                                <GaussianBlur radius="1.2" />
                            </effect>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="141.0" layoutY="11.0" prefHeight="100.0" prefWidth="158.0" styleClass="stat" stylesheets="@styles.css">
                    <children>
                        <Label fx:id="humidity_v" styleClass="stat-value" stylesheets="@styles.css" text="..." />
                        <ImageView fitWidth="42.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="-8.0">
                            <image>
                                <Image url="@../../../gfx/humidity.png" />
                            </image>
                        </ImageView>
                        <Label styleClass="stat-title" stylesheets="@styles.css" text="Humidity" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                            <effect>
                                <GaussianBlur radius="1.2" />
                            </effect>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="265.0" layoutY="11.0" prefHeight="100.0" prefWidth="158.0" styleClass="stat" stylesheets="@styles.css">
                    <children>
                        <Label fx:id="wind_v" styleClass="stat-value" stylesheets="@styles.css" text="..." />
                        <ImageView fitWidth="52.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="-2.0">
                            <image>
                                <Image url="@../../../gfx/wind.png" />
                            </image>
                        </ImageView>
                        <Label styleClass="stat-title" stylesheets="@styles.css" text="Wind" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                            <effect>
                                <GaussianBlur radius="1.2" />
                            </effect>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="507.0" layoutY="11.0" prefHeight="100.0" prefWidth="158.0" styleClass="stat" stylesheets="@styles.css">
                    <children>
                        <Label fx:id="visibility_v" styleClass="stat-value" stylesheets="@styles.css" text="..." />
                        <ImageView fitWidth="62.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="4.0" AnchorPane.topAnchor="8.0">
                            <image>
                                <Image url="@../../../gfx/visibility.png" />
                            </image>
                        </ImageView>
                        <Label styleClass="stat-title" stylesheets="@styles.css" text="Visibility" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                            <effect>
                                <GaussianBlur radius="1.2" />
                            </effect>
                        </Label>
                    </children>
                </AnchorPane>
            </children>
            <VBox.margin>
                <Insets left="20.0" right="20.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
