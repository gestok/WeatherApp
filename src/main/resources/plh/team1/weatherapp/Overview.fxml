<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox prefHeight="720.0" prefWidth="1024.0" spacing="20.0" styleClass="container" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="plh.team1.weatherapp.OverviewController">
    <children>
        <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="54.0" styleClass="navigation" stylesheets="@styles.css">
            <children>
                <Button fx:id="menuItemOverview" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="54.0" prefWidth="54.0">
                    <graphic>
                        <ImageView fitHeight="26.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../icons/report.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <styleClass>
                        <String fx:value="menu-item" />
                        <String fx:value="report" />
                        <String fx:value="active" />
                    </styleClass>
                </Button>
                <Button fx:id="menuItemSearch" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#switchToSearch" prefHeight="54.0" prefWidth="54.0" stylesheets="@styles.css">
                    <styleClass>
                        <String fx:value="menu-item" />
                        <String fx:value="cursor-pointer" />
                        <String fx:value="search" />
                    </styleClass>
                    <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../icons/search.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets left="20.0" right="20.0" />
            </padding>
            <rotationAxis>
                <Point3D />
            </rotationAxis>
        </HBox>
        <HBox alignment="CENTER" spacing="24.0" styleClass="cloud-overlay" stylesheets="@styles.css">
            <children>
                <FlowPane orientation="VERTICAL" prefHeight="200.0" prefWidth="0.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" fillHeight="false" maxHeight="30.0" prefHeight="30.0" prefWidth="150.0">
                            <children>
                                <Label fx:id="tempValue" text="25" textFill="#276487">
                                    <font>
                                        <Font name="System Bold" size="72.0" />
                                    </font>
                                </Label>
                                <Label fx:id="tempUnit" text="°C" textFill="#276487">
                                    <font>
                                        <Font size="26.0" />
                                    </font>
                                    <HBox.margin>
                                        <Insets bottom="30.0" />
                                    </HBox.margin>
                                </Label>
                            </children>
                            <FlowPane.margin>
                                <Insets />
                            </FlowPane.margin>
                        </HBox>
                        <Label fx:id="weatherDesc" text="Cloudy" textFill="#276487">
                            <font>
                                <Font size="16.0" />
                            </font>
                            <FlowPane.margin>
                                <Insets top="-20.0" />
                            </FlowPane.margin>
                        </Label>
                        <HBox alignment="CENTER_LEFT" spacing="6.0">
                            <children>
                                <ImageView fitHeight="22.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../../icons/location.png" />
                                    </image>
                                </ImageView>
                                <Label fx:id="location" text="Milan, Italy" textFill="#276487">
                                    <font>
                                        <Font name="System Bold" size="24.0" />
                                    </font>
                                </Label>
                            </children>
                            <FlowPane.margin>
                                <Insets top="-2.0" />
                            </FlowPane.margin>
                        </HBox>
                        <Label fx:id="highLowFeels" text="26° / 20° Feels like 30" textFill="#276487">
                            <font>
                                <Font size="20.0" />
                            </font>
                            <FlowPane.margin>
                                <Insets top="10.0" />
                            </FlowPane.margin>
                        </Label>
                        <Label fx:id="currentDate" text="Fri, 02:30 am" textFill="#276487">
                            <font>
                                <Font size="20.0" />
                            </font>
                        </Label>
                    </children>
                </FlowPane>
                <FlowPane alignment="CENTER_LEFT" prefWidth="300.0" HBox.hgrow="ALWAYS">
                    <children>
                        <ImageView fitHeight="220.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../gfx/states/snowy-thunder.png" />
                            </image>
                        </ImageView>
                    </children>
                </FlowPane>
                <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="24.0" prefHeight="200.0" prefWidth="340.0" prefWrapLength="200.0" vgap="24.0">
                    <children>
                        <AnchorPane prefHeight="100.0" prefWidth="158.0" styleClass="stat" stylesheets="@styles.css">
                            <children>
                                <Label fx:id="uvindex_v" styleClass="stat-value" stylesheets="@styles.css" text="..." />
                                <ImageView fitWidth="55.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="-10.0">
                                    <image>
                                        <Image url="@../../../gfx/uvindex.png" />
                                    </image>
                                </ImageView>
                                <Label styleClass="stat-title" stylesheets="@styles.css" text="UV Index" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                                    <effect>
                                        <GaussianBlur radius="1.2" />
                                    </effect>
                                </Label>
                            </children>
                        </AnchorPane>
                        <AnchorPane prefHeight="100.0" prefWidth="158.0" styleClass="stat" stylesheets="@styles.css">
                            <children>
                                <Label fx:id="humidity_v" styleClass="stat-value" stylesheets="@styles.css" text="..." />
                                <ImageView fitWidth="45.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="18.0" AnchorPane.topAnchor="-2.0">
                                    <image>
                                        <Image url="@../../../gfx/humidity.png" />
                                    </image>
                                </ImageView>
                                <Label styleClass="stat-title" stylesheets="@styles.css" text="Humidity" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                                    <effect>
                                        <GaussianBlur radius="1.2" />
                                    </effect>
                                </Label>
                            </children>
                        </AnchorPane>
                        <AnchorPane prefHeight="100.0" prefWidth="158.0" styleClass="stat" stylesheets="@styles.css">
                            <children>
                                <Label fx:id="wind_v" styleClass="stat-value" stylesheets="@styles.css" text="..." />
                                <ImageView fitWidth="53.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="-4.0">
                                    <image>
                                        <Image url="@../../../gfx/wind.png" />
                                    </image>
                                </ImageView>
                                <Label styleClass="stat-title" stylesheets="@styles.css" text="Wind" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                                    <effect>
                                        <GaussianBlur radius="1.2" />
                                    </effect>
                                </Label>
                            </children>
                        </AnchorPane>
                        <AnchorPane prefHeight="100.0" prefWidth="158.0" styleClass="stat" stylesheets="@styles.css">
                            <children>
                                <Label fx:id="visibility_v" styleClass="stat-value" stylesheets="@styles.css" text="..." />
                                <ImageView fitWidth="52.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="-10.0">
                                    <image>
                                        <Image url="@../../../gfx/visibility.png" />
                                    </image>
                                </ImageView>
                                <Label styleClass="stat-title" stylesheets="@styles.css" text="Visibility" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                                    <effect>
                                        <GaussianBlur radius="1.2" />
                                    </effect>
                                </Label>
                            </children>
                        </AnchorPane>
                    </children>
                </FlowPane>
            </children>
            <VBox.margin>
                <Insets left="20.0" right="20.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
